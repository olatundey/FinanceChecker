@model List<FinanceChecker.Models.FAQ>

@{
    ViewData["Title"] = "Frequently Asked Questions";
}
<br />
<h1>Frequently Asked Questions</h1>

@if (Model.Count > 0)
{
    <ul>
        @foreach (var faq in Model)
        {
            <li>
                <strong>@faq.Question</strong>
                <p>@faq.Answer</p>
            </li>
        }
    </ul>
    <br />

}
else
{
    <p>No FAQs found.</p>
}
<script></script>
